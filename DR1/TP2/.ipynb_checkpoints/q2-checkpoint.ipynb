   "source": [
    "def selection_sort(a):\n",
    "    n = len(a)\n",
    "    for i in range(n - 1):\n",
    "        min_idx = i\n",
    "        for j in range(i + 1, n):\n",
    "            if a[j] < a[min_idx]:\n",
    "                min_idx = j\n",
    "        if min_idx != i:\n",
    "            a[i], a[min_idx] = a[min_idx], a[i]\n",
    "    return a"
   ]
  }
